{
  "name": "papersfm",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "start:aot": "ng serve --aot",
    "start:android": "cd apps/mobile && tns run android --emulator",
    "build": "rimraf dist/apps/web && ng build",
    "build:dev": "npm run build -- --prod --env=dev",
    "build:dev:aot": "npm run build:dev -- --aot",
    "build:dev:ci": "npm-run-all -p -r build:dev build:dev:aot",
    "build:stag": "npm run build -- --prod --env=stag",
    "build:stag:aot": "npm run build:stag -- --aot",
    "build:stag:ci": "npm-run-all -p -r build:stag build:stag:aot",
    "build:prod": "npm run build -- --prod --env=prod",
    "build:prod:aot": "npm run build:prod -- --aot",
    "build:sourcemaps": "npm run build:prod:aot -- --vendor-chunk=false --sourcemaps",
    "build:prod:ci": "npm-run-all -p -r build:prod build:prod:aot",
    "clean": "rimraf node_modules && npm cache clean --force",
    "test": "ng test -sr",
    "test:coverage": "rimraf coverage && npm run test -- -cc",
    "lint": "ng lint --type-check",
    "lint:fix": "ng lint --fix",
    "lint:styles": "stylelint **/*.scss !**/node_modules/**",
    "lint:ci": "npm run lint:styles && npm run lint",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "prepush": "npm run lint:ci",
    "precommit": "npm run lint:ci",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "conventional-changelog": "conventional-changelog",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.0",
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@angular/router": "5.0.0",
    "@nebular/theme": "^2.0.0-rc.3",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@ngrx/effects": "^4.1.0",
    "@ngrx/router-store": "^4.1.0",
    "@ngrx/store": "^4.1.0",
    "@ngrx/store-devtools": "4.0.0",
    "@nrwl/nx": "0.2.0",
    "angular2-toaster": "^4.0.1",
    "angularfire2": "^5.0.0-rc.3",
    "bootstrap": "^4.0.0-beta.2",
    "core-js": "^2.4.1",
    "firebase": "^4.6.2",
    "font-awesome": "^4.7.0",
    "ionicons": "^2.0.1",
    "nebular-icons": "^1.0.6",
    "pace-js": "^1.0.2",
    "raven-js": "^3.20.1",
    "roboto-fontface": "^0.8.0",
    "rxjs": "^5.5.2",
    "socicon": "^3.0.5",
    "typeface-exo": "0.0.44",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "nrwl/fix-cli-build",
    "@angular/compiler-cli": "5.0.0",
    "@angular/language-service": "5.0.0",
    "@compodoc/compodoc": "^1.0.4",
    "@nrwl/schematics": "0.2.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "conventional-changelog-cli": "^1.3.5",
    "firebase-tools": "^3.15.2",
    "husky": "^0.14.3",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.2",
    "stylelint": "^8.2.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "tslint-junit-formatter": "^5.1.0",
    "tslint-language-service": "^0.9.6",
    "typescript": "^2.4.2"
  }
}
